# Dashboard Configuration
# Copy this file to .env and fill in your values

# Required: Dashboard authentication token
DASHBOARD_TOKEN=your-secure-token-here

# MCP Server URL (for production, use service name: http://mcp:8000)
# For local dev with host machine: http://host.docker.internal:8000
MCP_SERVER_URL=http://mcp:8000

# Server configuration
PORT=5050
HOST=0.0.0.0

# CORS origins (comma-separated)
CORS_ORIGINS=*

# Flask secret key (auto-generated if not set)
SECRET_KEY=
